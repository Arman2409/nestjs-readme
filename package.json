{
  "name": "nestjs-readme",
  "version": "0.1.1",
  "description": "Generate README files automatically for Nest.js projects",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsup ./src/index.ts",
    "build:watch": "tsup ./src/index.ts --watch"
