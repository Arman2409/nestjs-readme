  },
  "keywords": ["Nest.js", "README", "documentation"],
  "author": "Arman Ghazaryan",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.14.9",
    "tsup": "^8.1.0"
  }
}
